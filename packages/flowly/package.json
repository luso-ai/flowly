{
  "name": "@luso-ai/flowly",
  "version": "0.1.0",
  "description": "Typed flows, not routes.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./devtools": {
      "types": "./dist/devtools/index.d.ts",
      "import": "./dist/devtools/index.js",
      "require": "./dist/devtools/index.cjs"
    },
    "./url": {
      "types": "./dist/url/index.d.ts",
      "import": "./dist/url/index.js",
      "require": "./dist/url/index.cjs"
    },
    "./test": {
      "types": "./dist/test/index.d.ts",
      "import": "./dist/test/index.js",
      "require": "./dist/test/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "dependencies": {
    "mermaid": "^10.9.1"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "lint": "tsc -p tsconfig.json --noEmit"
  }
}
